{
  "name": "vuipeilu",
  "version": "0.1.9",
  "description": "vui-自定义组件库",
  "main": "componentsdist/index.umd.js",
  "keywords": [
    "vui组件库",
    "vue",
    "ui"
  ],
  "author": "pei_lu",
  "type": "commonjs",
  "files": [
    "componentsdist",
    "components"
  ],
  "license": "ISC",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build:js": "rimraf componentsdist && npm rebuild node-sass && webpack --config ./webpack.components.js",
    "build:css": "npx gulp sass",
    "build:com": "rimraf componentsdist && npm rebuild node-sass && webpack --config ./webpack.components.js",
    "lint": "vue-cli-service lint",
    "clean": "rimraf node_modules && npm cache clear --force",
    "docs:dev": "npm rebuild node-sass && vuepress dev docs --temp .temp",
    "docs:build": "npm rebuild && vuepress build docs",
    "deploy": "bash deploy.sh"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.10",
    "querystringify": "^2.2.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.3",
    "vuepress": "^1.9.7",
    "vuex": "3",
    "vuipeilu": "^0.1.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.19",
    "@vue/cli-plugin-eslint": "~4.5.19",
    "@vue/cli-service": "~4.5.19",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^5.1.0",
    "node-sass": "^4.14.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^7.0.1",
    "sass": "^1.55.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^3.3.1",
    "style-resources-loader": "^1.5.0",
    "vue-loader": "^15.7.0",
    "vue-loader-plugin": "^1.3.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^4.10.0"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
